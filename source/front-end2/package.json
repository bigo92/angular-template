{
  "name": "front-end",
  "version": "0.0.0",
  "scripts": {
    "start": "concurrently npm:start:*",
    "start:shell": "ng serve --project=front-end --port 4200",
    "start:dashboard": "ng serve --project=dashboard --port 4201",
    "start:sso": "ng serve --project=sso --port 4202",
    "build": "concurrently -m 3 -n w: npm:build:*",
    "build:shell": "ng build --project=front-end",
    "build:mailbox": "ng build --project=mailbox",
    "build:calendar": "ng build --project=calendar"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "ng-zorro-antd": "~12.1.1",
    "rxjs": "~6.6.0",
    "tslib": "~2.3.0",
    "zone.js": "~0.11.4",
    "inputmask": "~5.0.6",
    "angular2-text-mask": "9.0.0",
    "text-mask-addons": "~3.8.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "~12.1.3",
    "@angular-devkit/build-angular": "~12.2.16",
    "@angular/cli": "~12.2.16",
    "@angular/compiler-cli": "~12.2.0",
    "@types/inputmask": "^5.0.3",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5"
  }
}
